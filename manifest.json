{
  "manifest_version": 3,
  "name": "YTAdblockSafari",
  "version": "1.0",
  "description": "Automatically skips ads on YouTube.",
  "permissions": [
    "scripting"
  ],
  "host_permissions": [
    "*://www.youtube.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}